{
  "name": "deckforge",
  "version": "1.0.0",
  "description": "student project for Wild Code School",
  "keywords": [],
  "authors": [],
  "license": "MIT",
  "private": true,
  "config": {
    "cli": "npm"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "echo '----- Checking Frontend...' && prettier --debug-check . && eslint --ext .jsx . && echo '----- Done!'",
    "format": "prettier --write . && eslint --fix --ext .jsx .",
    "docker:build": "dotenv -- cross-var docker build --build-arg VITE_HS_API_KEY=%VITE_HS_API_KEY% --tag deckforge:latest --progress plain .",
    "docker:run": "docker run --name deckforge -p 4000:80 -d deckforge:latest",
    "docker:stop": "docker stop deckforge",
    "docker:remove": "docker container rm deckforge && docker image rm deckforge"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@vitejs/plugin-react": "^3.0.1",
    "axios": "^1.3.5",
    "deckstrings": "^2.2.1",
    "hamburgers": "^1.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.10.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.2",
    "slick-carousel": "^1.8.1",
    "vite": "^4.0.4"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "sass": "^1.62.0"
  }
}
